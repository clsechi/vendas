<%= form_tag('/create_seller', method: :post) do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= text_field_tag :password %>
  </div>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div>
    <%= submit_tag 'Criar' %>
  </div>
<% end %>
