<%= form_for(@seller, url: {action: 'create_seller'}) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :password, 'Senha' %>
    <%= form.text_field :password %>
  </div>
  <div>
    <%= form.label :name, 'Nome' %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.submit 'Criar' %>
  </div>
<% end %>
